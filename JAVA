public class CAR  {
  string name ;
  string color ;
  short speed ;
  

 void start () {
 system.out.println(name +"starting");
 }
 void stop () {
 system.out.println(name +"stoping");
 }
 void changegear () {
 system.out.println(name +"changing gear");
 }
