public class PLAYER  {
  string name ;
  short age ;
  short rank ;

 void pass () {
 system.out.println(name +"Passing");
 }
 void shoot () {
 system.out.println(name +"shooting");
 }
 void jump () {
 system.out.println(name +"jumping");
 }
public class Main {
public static void man (string[] args) {
PLAYER firstplayer = New player() ;
firstplayer.name="AYMAN";
firstplayer.age=30;
firstplayer.rank=1;
firstplayer.pass();

