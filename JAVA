public class PLAYER  {
  string name ;
  short age ;
  short rank ;

 void pass () {
 system.out.println(name +"Passing");
 }
 void shoot () {
 system.out.println(name +"shooting");
 }
 void jump () {
 system.out.println(name +"jumping");
 }
