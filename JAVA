public class MOBILE  {
  string name ;
  long serialname ;
  short storage ;
  

 void call () {
 system.out.println(name +"is calling");
 }
 void send () {
 system.out.println(name +"is sending a msg");
 }
 void play () {
 system.out.println(name +"playing a media");
 }
